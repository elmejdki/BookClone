{
  "clock.active": true,

	// Note: You must reload to window to see changes.
	"clock.alignment": "Right",

	// See https://www.npmjs.com/package/dateformat#mask-options for more formatting options.
	"clock.format": "hh:MM:ss",

	// See https://octicons.github.com/ for a icon list. Use an unsupported icon to disable the icon.
	"clock.iconName": "cpu",

	// Note: You must reload to window to see changes.
	"clock.priority": 30,

	// The clock update interval in milliseconds.
	"clock.updateInterval": 2000,

}